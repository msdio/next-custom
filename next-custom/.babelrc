{
  "presets": [
    "next/babel",
    {
      "preset-env": {
        "targets": {
          "esmodules": true,
          "browsers": ["last 2 versions"]
        }
      },
      "preset-react": {
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    }
  ],
  "plugins": ["@babel/plugin-proposal-do-expressions", "@emotion"]
}
